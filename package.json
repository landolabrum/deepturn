{
  "name": "deept",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "deploy": "npm run export && touch ./out/.nojekyll && touch out/CNAME && echo \"deepturn.com\" >> out/CNAME && git add out/  && git commit -m \"Deploy to gh-pages\" && gh-pages -d out -t true",
